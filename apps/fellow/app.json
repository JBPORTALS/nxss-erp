{
  "expo": {
    "scheme": "fellow",
    "icon": "./assets/icon.png",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "OpenSans": "./assets/fonts/OpenSans.ttf"
        }
      ]
    ],
    "name": "fellow",
    "slug": "fellow",
    "userInterfaceStyle": "automatic",
    "android": {
      "splash": {
        "image": "./assets/splash-screen.png",
        "backgroundColor": "#ffffff"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  }
}
