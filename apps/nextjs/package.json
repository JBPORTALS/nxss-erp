{
  "name": "@nxss/nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@nxss/api": "workspace:^",
    "@nxss/db": "workspace:^",
    "@nxss/ui": "workspace:^",
    "@nxss/validators": "workspace:^",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.51.1",
    "@tanstack/react-query-devtools": "^5.51.1",
    "@tanstack/react-query-next-experimental": "5.51.1",
    "@tanstack/react-table": "^8.19.2",
    "@trpc/client": "11.0.0-rc.459",
    "@trpc/react-query": "11.0.0-rc.459",
    "@trpc/server": "11.0.0-rc.459",
    "date-fns": "^3.6.0",
    "geist": "^1.3.1",
    "lucide-react": "^0.408.0",
    "next": "^14.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nxss/eslint-config": "workspace:^",
    "@nxss/prettier-config": "workspace:^",
    "@nxss/tailwind-config": "workspace:^",
    "@nxss/tsconfig": "workspace:^",
    "@types/node": "^20.14.10",
    "@types/react": "~18.2.79",
    "@types/react-dom": "^18.3.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.56.0",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": " ~5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@nxss/eslint-config/base",
      "@nxss/eslint-config/nextjs",
      "@nxss/eslint-config/react"
    ]
  },
  "prettier": "@nxss/prettier-config"
}
