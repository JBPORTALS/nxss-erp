{
  "name": "@nxss/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./schema": "./src/schema/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "push": "pnpm with-env drizzle-kit push",
    "up": "pnpm with-env drizzle-kit up",
    "check": "pnpm with-env drizzle-kit check",
    "drop": "pnpm with-env drizzle-kit drop",
    "generate": "pnpm with-env drizzle-kit generate",
    "migrate": "pnpm with-env drizzle-kit migrate",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "seed": "pnpm with-env dotenv -v SEED_MODE=true tsx src/seed.ts"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^5.0.42",
    "@faker-js/faker": "^8.4.1",
    "@neondatabase/serverless": "^0.9.5",
    "@paralleldrive/cuid2": "^2.2.2",
    "@planetscale/database": "^1.18.0",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.35.1",
    "drizzle-zod": "^0.6.1",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nxss/eslint-config": "workspace:^",
    "@nxss/prettier-config": "workspace:^",
    "@nxss/tsconfig": "workspace:^",
    "@types/pg": "^8.11.6",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.26.2",
    "eslint": "^8.56.0",
    "prettier": "^3.3.2",
    "tsx": "^4.16.5",
    "typescript": " ~5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@nxss/eslint-config/base"
    ]
  },
  "prettier": "@nxss/prettier-config"
}
